{
  "name": "be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node src/server.js",
    "clean": "rimraf ./dist",
    "build": "npm run clean && babel src -d dist --copy-files",
    "test": "jest --config jest.config.json",
    "test:unit": "jest --config jest.config.json",
    "test:coverage": "jest --coverage --config jest.config.json",
    "db:re-index": "babel-node ./src/db-cli.js re-index",
    "db:seed": "babel-node ./src/db-cli.js seed",
    "db:migrate": "babel-node ./src/db-cli.js migrate",
    "dist:db:re-index": "node -r dotenv/config dist/db-cli re-index",
    "dist:db:seed": "node -r dotenv/config dist/db-cli seed",
    "dist:db:migrate": "node -r dotenv/config dist/db-cli migrate",
    "docker:image": "./scripts/docker-image.sh",
    "setup": "bash scripts/setup.sh"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "helmet": "^5.0.2",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "pg": "^8.7.3",
    "xss-clean": "^0.1.1"
  }
}